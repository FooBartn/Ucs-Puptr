<command:details><command:name>Invoke-PuptrTest</command:name>
<command:verb>Invoke</command:verb>
<command:noun>PuptrTest</command:noun>
<maml:description><maml:para>Provide an extremely light-weight approach to Cisco UCS configuration management
</maml:para>
</maml:description>
</command:details>
<maml:description><maml:para>Utilize Pester and Cisco UCS PowerTool to provide a set if Operation Validation tests with the option to remediate if applicable.
</maml:para>
</maml:description>
<command:syntax><command:syntaxItem><maml:name>Invoke-PuptrTest</maml:name>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="1" aliases="none"><maml:name>TestPath</maml:name>
<maml:Description><maml:para>Directory of the tests that you want to run. Default = '.\Ucs-Puptr'
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String</command:parameterValue>
<dev:type><maml:name>String</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>"$PSScriptRoot\..\Diagnostics"</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="2" aliases="none"><maml:name>ConfigName</maml:name>
<maml:Description><maml:para>Location of the configuration file you want to use. Default = 'Config'
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String</command:parameterValue>
<dev:type><maml:name>String</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>Config</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="3" aliases="none"><maml:name>TestName</maml:name>
<maml:Description><maml:para>Informs Invoke-Pester to only run Describe blocks that match this name.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>None</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="4" aliases="none"><maml:name>Tag</maml:name>
<maml:Description><maml:para>Informs Invoke-Pester to only run Describe blocks tagged with the tags specified. Aliased 'Tags' for backwards compatibility.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
